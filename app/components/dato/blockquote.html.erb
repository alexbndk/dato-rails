<figure class="dato-cms-figure">
  <%= content_tag generated_tag, class: "dato-cms-#{@node.type}" do %>
    <% @node.children&.each do |node| %>
      <%= render_node(node) %>
    <% end %>
  <% end %>

  <figcaption class="dato-cms-attribution">
    <%= @node.attribution %>
  </figcaption>
</figure>
